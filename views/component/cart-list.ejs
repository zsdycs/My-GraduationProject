<div class="content-main--basic cart-list" id="content" style="display:none">

    <div class="">
        <section class="page-section" data-view="cartPage">
            <div class="grid-container">
                <section class="layout -sidebar-right">
                    <div class="layout__main-content">
                        <div class="cart-header">
                            <span class="cart-header__left">
                                <a class="btn" href="/">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">继续购物</font>
                                    </font>
                                </a>
                            </span>
                            <span class="cart-header__summary"></span>
                            <dic class="cart-header__right">
                                <button name="button" type="submit" class="e-btn -color-dark" id="del-all">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">清空</font>
                                    </font>
                                </button>

                            </dic>
                        </div>

                        <div class="shopping-cart" id="cartList">
                            <div v-cloak v-for="item in cartList" class="shopping-cart__group">
                                <article class="shopping-cart__entry">
                                    <div class="item-result">


                                        <div class="item-result__details">
                                            <div class="cart-entry">

                                                <div class="cart-entry__title">
                                                    <h5 class="t-body -weight-bold h-m0">{{item.name}}</h5>
                                                </div>



                                                <div class="cart-entry__information">
                                                    <div class="cart-entry__license">
                                                        <p class="t-body -size-s h-m0">
                                                            <strong>许可证：</strong>
                                                            <a class="cart_entry__information-detail">{{item.license}}</a>
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>


                                            <div class="item-result__price">
                                                <div class="cart-entry__remove">
                                                    <button v-bind:data-audio_id="item.id" name="button" class="btn-link"><i class="x icon"></i></button>
                                                </div>
                                                <div class="cart-entry__price">
                                                    <h6 class="t-heading -size-l h-m0">
                                                        <b class="js-item-upgrade__item-price t-currency">
                                                            <font style="vertical-align: inherit;">
                                                                <font style="vertical-align: inherit;">
                                                                    ￥<span class="price-one">{{item.price}}</span>
                                                                </font>
                                                            </font>
                                                        </b>
                                                    </h6>
                                                </div>
                                            </div>
                                        </div>

                                       
                                    </div>

                                </article>
                            </div>

                            <footer class="cart-footer is-hidden-desktop">
                                <div class="cart-footer__subtotal">
                                    <span class="t-heading -size-s -weight-normal">
                                        <font style="vertical-align: inherit;">
                                            <font style="vertical-align: inherit;">总计:</font>
                                        </font>
                                    </span>
                                    <span class="t-heading -size-l">
                                        <font style="vertical-align: inherit;">
                                            <font style="vertical-align: inherit;">￥<span id="total-price">0</span></font>
                                        </font>
                                        </span>
                                    </span>
                                    <span class="cart-footer__checkout">
                                        <button class="ui secondary button">结 算</button>
                                    </span>
                                </div>
                            </footer>

                        </div>
                    </div>


                </section>
            </div>
        </section>


    </div>
</div>
<script src="/javascripts/for_cartList.js"></script>