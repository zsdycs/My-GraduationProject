<!DOCTYPE html>
<html lang="zh">

<head>
  <title><%= title %> </title>

  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width,initial-scale=1">

  <link rel="stylesheet" href="/stylesheets/for_wave_row.css">

  <link rel="stylesheet" type="text/css" href="/stylesheets/semantic-ui/semantic.min.css">
  <link rel="stylesheet" href="/stylesheets/index.css">

  <script src="/javascripts/lib/jquery-1.11.1.min.js"></script>
  <script src="/javascripts/lib/jquery-migrate-1.1.1.js"></script>
  <script src="/javascripts/lib/vue2-3-4.js"></script>
  <script src="/javascripts/semantic-ui/semantic.min.js"></script>
  <script src="/javascripts/common.js"></script>
  <script src="/javascripts/index.js"></script>
  
</head>

<body class="pushable">

<!-- Following header -->
<%- include('./following-header') %>

<!-- Sidebar Menu -->
<%- include('./sidebar-menu') %>


<!-- Page Contents -->
<div class="pusher">
  <%- include('./index-contents') %>

  <div class="two column stackable ui grid">
    <div class="three wide column">
      <%- include('./filtrate') %>
    </div>
    <div class="thirteen wide column">
      <div class="ui mini menu">
        <!-- 标签组 -->
      <div class="ui labels">
        <p class="_1X0WU"><span class="_3HYTF">53200</span>条</p>
        <div id="labeltagList">
          <a v-cloak v-for="item in labeltagList" class="ui label labeltag loader">{{item.tag_name}}<i class="delete icon"></i></a>
        </div>
      </div>
      <script>
      </script>
        <!-- 右边下拉列表（排序） -->
        <div class="right menu">
          <div class="ui dropdown item">
            <div class="dropdown-title">综合排序</div>
            <i class="dropdown icon"></i>
            <div class="menu">
              <a class="rank-a item active selected">综合排序</a>
              <a class="rank-a item">销售最高</a>
              <a class="rank-a item">价格最低</a>
              <a class="rank-a item">日期最新</a>
            </div>
          </div>
        </div>
      </div>
      <!-- 音频片段 -->
      <%- include('./audioList') %>
      <!-- 分页按钮 -->
      <%- include('../component/pageButton') %>
    </div>
  </div>
    
      

      